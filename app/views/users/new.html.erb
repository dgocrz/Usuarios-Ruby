 <%= stylesheet_link_tag 'style.css' %>

<h1>Registro de usuarios</h1>
<p>Find me in app/views/users/new.html.erb</p>
<%= form_with model: @user do |f| %>
<p><%= f.label :name %><br><%= f.text_field :name %></p>
  <p><%= f.label :email %><br><%= f.email_field :email %></p>
  <p><%= f.label :password %><br><%= f.password_field :password %></p>
  <p><%= f.label :password_confirmation %><br><%= f.password_field :password_confirmation %></p>
  <p><%= f.submit "Crear usuario" %></p>

    <% end %>

    